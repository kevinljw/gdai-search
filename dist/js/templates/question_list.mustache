<div>
   <p>{{num}} results  ( {{timeDiff}} seconds )</p>
   <div class="card">
    <div class="card-body table-responsive" style="height: 700px;">
       <table class="table table-hover table-head-fixed table-valign-middle">
          <thead>
              <tr>
                <th>#Q</th>
                <th>Risk areas</th>
                <th>Score</th>
                <th>Country</th>
                <th>Band</th>
                <th>More</th>
              </tr>
          </thead>
          <tbody>
            {{#questions}}
                <tr data-question-id="{{id}}" style="line-height:3rem;">
                        <td>
                            {{qn}}
                        </td>
                        <td>
                            {{facets}}
                        </td>
                        <td>
                            {{score}}
                        </td>
                         <td>
                            {{country}}
                        </td>
                         <td>
                            {{band}}
                        </td>
                         <td class="btn-group">
                            <a class="btn btn-default">
                              <i class="fas fa-eye"></i>
                            </a>
                        </td>
                </tr>
            {{/questions}}
          <tbody>
        </table>
     </div>
  </div>
</div>
